@model List<CheeseMVC.Models.CheeseCategory>
@{
    ViewBag.Title = "Categories";
}

<h1>@ViewBag.Title</h1>
<ul>
    @foreach (var category in Model)
    {
        <li>@category.Name</li>

    }
</ul>
<p><a asp-controller="Category" asp-action="Add">Add</a> | <a asp-controller="Category" asp-action="Remove">Remove</a></p>

